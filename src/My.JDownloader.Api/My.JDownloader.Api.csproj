<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net46;netcoreapp31;net8.0;net9.0</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Cr1TiKa7</Authors>
    <Product />
    <Description>A simple My.JDownloader Api wrapper for c# written by Cr1TiKa7</Description>
    <Copyright>Cr1TiKa7 Â©2018 - 2022</Copyright>
    <PackageLicense>https://github.com/Cr1TiKa7/My.Jdownloader.Api/blob/master/LICENSE</PackageLicense>
    <PackageProjectUrl>https://github.com/Cr1TiKa7/My.Jdownloader.Api</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Cr1TiKa7/My.Jdownloader.Api</RepositoryUrl>
    <Version>2.4.6</Version>
    <PackageReleaseNotes></PackageReleaseNotes>
    <IconUrl>https://i.imgur.com/sJWbszc.png</IconUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>
	<ItemGroup Condition=" '$(TargetFramework)' == 'net46' ">
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Web" />
    <Reference Include="System.Runtime"/>
  </ItemGroup>
</Project>